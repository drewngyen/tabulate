{
    "name": "tabulate",
    "version": "0.3",
    "description": "Your personal and private tab manager.",
    "background": {
      "persistent": false,
      "scripts": ["main.js"]
    },
    "permissions": [
        "tabs"
    ],

    "commands": {
      "toggle-pin": {
        "suggested_key": { "default": "Ctrl+Shift+F" },
        "description": "Toggle tab pin"
      },
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Alt+O"
        }
      }
    },
    "browser_action": {
        "default_icon": "tabulate128.png",
        "default_title": "test popup",
        "default_popup": "popup.html"
    },
    "manifest_version": 2
  }
  