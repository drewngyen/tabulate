{
  "name": "tabulate",
  "version": "0.3",
  "description": "Creates a keyboard shortcut (Alt + O), (Option + O) on Mac to toggle the pinned state of the currently selected tab",
  "background": {
    "persistent": false,
    "scripts": ["main.js"]
  },
  "permissions": ["tabs", "storage"],

  "commands": {
    "toggle-pin": {
      "suggested_key": { "default": "Ctrl+Shift+F" },
      "description": "Toggle tab pin"
    },
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+O"
      }
    }
  },
  "browser_action": {
    "default_icon": "tabulate128.png",
    "default_title": "test popup",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
