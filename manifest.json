{
  "name": "tabulate",
  "version": "0.1",
  "description": "Your personal tab manager. Press OPTION-O to get started! Search and organize your open tabs.",
  "background": {
    "persistent": false,
    "scripts": ["main.js"]
  },
  "permissions": ["tabs", "storage"],

  "commands": {
    "toggle-pin": {
      "suggested_key": { "default": "Ctrl+Shift+F" },
      "description": "Toggle tab pin"
    },
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+O"
      }
    }
  },
  "browser_action": {
    "default_icon": "tabulate128.png",
    "default_title": "test popup",
    "default_popup": "popup.html"
  },
  "icons" : {
    "128" : "tabulate128.png"
  },
  "manifest_version": 2
}
