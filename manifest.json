{
    "name": "Esther and Drew's extensiono",
    "version": "0.3",
    "description": "Creates a keyboard shortcut (Alt + O), (Option + O) on Mac to toggle the pinned state of the currently selected tab",
    "background": {
      "persistent": false,
      "scripts": ["main.js"]
    },
    "permissions": [
        "tabs"
    ],

    "commands": {
      "toggle-pin": {
        "suggested_key": { "default": "Alt+O" },
        "description": "Toggle tab pin"
      }
    },
    "browser_action": {
        "default_title": "test popup",
        "default_popup": "popup.html"
    },
    "manifest_version": 2
  }
  